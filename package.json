{
  "name": "cravate-doree",
  "version": "2.0.0",
  "description": "Typescript with React, using parcel bundler",
  "homepage": "https://thoroc.github.io/cravate-doree",
  "repository": {
    "type": "git",
    "url": "https://github.com/thoroc/cravate-doree"
  },
  "author": "Thomas Roche",
  "license": "MIT",
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "scripts": {
    "build": "yarn type-check && vite build",
    "deploy": "gh-pages -d build",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "prebuild": "yarn lint && yarn format",
    "predeploy": "yarn build",
    "serve": "vite preview",
    "test:coverage": "jest --coverage",
    "test:all": "yarn test:coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "vite": "^4.3.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.4.1",
    "@types/node": "^20.3.2",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-testing-library": "^5.11.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-extended": "^4.0.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.9.5",
    "vite-plugin-eslint": "^1.8.1"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styleMock.ts"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
