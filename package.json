{
  "name": "cravate-doree",
  "version": "2.0.0",
  "description": "Typescript with React, using parcel bundler",
  "homepage": "https://thoroc.github.io/cravate-doree",
  "repository": {
    "type": "git",
    "url": "https://github.com/thoroc/cravate-doree"
  },
  "author": "Thomas Roche",
  "license": "MIT",
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "type": "module",
  "scripts": {
    "build": "wireit",
    "deploy": "wireit",
    "dev": "wireit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "prebuild": "wireit",
    "predeploy": "wireit",
    "serve": "wireit",
    "test:coverage": "wireit",
    "test:all": "wireit",
    "test:watch": "wireit",
    "type-check": "wireit",
    "postinstall": "wireit",
    "prepack": "wireit",
    "postpack": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "type-check"
      ],
      "command": "vite build"
    },
    "deploy": {
      "command": "gh-pages -d build"
    },
    "dev": {
      "command": "vite"
    },
    "prebuild": {
      "dependencies": [
        "lint",
        "format"
      ]
    },
    "predeploy": {
      "dependencies": [
        "build"
      ]
    },
    "serve": {
      "command": "vite preview"
    },
    "test:coverage": {
      "command": "jest --coverage"
    },
    "test:all": {
      "dependencies": [
        "test:coverage"
      ]
    },
    "test:watch": {
      "command": "jest --watch"
    },
    "type-check": {
      "command": "tsc --noEmit"
    },
    "type-check:watch": {
      "command": "tsc --noEmit --watch"
    },
    "postinstall": {
      "command": "husky install"
    },
    "prepack": {
      "command": "pinst --disable"
    },
    "postpack": {
      "command": "pinst --enable"
    }
  },
  "dependencies": {
    "konva": "^9.3.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-konva": "^18.2.10",
    "vite": "^5.4.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-testing-library": "^6.3.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.1.6",
    "inquirer": "^11.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "pinst": "^3.0.0",
    "prettier": "^3.3.3",
    "process": "^0.11.10",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite-plugin-eslint": "^1.8.1",
    "wireit": "^0.14.9"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
