{
  "name": "ts-react-parcel",
  "version": "2.0.0",
  "description": "Typescript with React, using parcel bundler",
  "repository": {
    "type": "git",
    "url": "https://github.com/adhrinae/ts-react-parcel"
  },
  "author": "Ahn Dohyung",
  "license": "MIT",
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "scripts": {
    "build": "npm run type-check && parcel build ./src/index.html --public-url '/'",
    "develop": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "test:coverage": "jest --coverage",
    "test:all": "yarn test:coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "concurrently": "^4.1.0",
    "eslint": "^8.10.0",
    "eslint-plugin-react": "^7.29.3",
    "husky": "^7.0.0",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "jest-extended": "^0.11.1",
    "parcel": "^2.3.2",
    "pinst": "^3.0.0",
    "prettier": "^2.5.1",
    "react-test-renderer": "^16.13.0",
    "react-testing-library": "^6.1.2",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.4.4"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styleMock.ts"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "src/**/*.{css,json,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
