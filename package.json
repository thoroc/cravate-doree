{
  "name": "ts-react-parcel",
  "version": "2.0.0",
  "description": "Typescript with React, using parcel bundler",
  "repository": {
    "type": "git",
    "url": "https://github.com/adhrinae/ts-react-parcel"
  },
  "author": "Ahn Dohyung",
  "license": "MIT",
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "scripts": {
    "build": "yarn type-check && parcel build ./public/index.html --public-url '/'",
    "develop": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "prebuild": "yarn lint && yarn format",
    "start": "parcel ./src/index.html --public-url '/'",
    "test:coverage": "jest --coverage",
    "test:all": "yarn test:coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.26",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.14.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-testing-library": "^5.1.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "parcel": "^2.4.1",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.3"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styleMock.ts"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
