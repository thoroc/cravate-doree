{
  "name": "cravate-doree",
  "version": "2.0.0",
  "description": "Typescript with React, using parcel bundler",
  "homepage": "https://thoroc.github.io/cravate-doree",
  "repository": {
    "type": "git",
    "url": "https://github.com/thoroc/cravate-doree"
  },
  "author": "Thomas Roche",
  "license": "MIT",
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "type": "module",
  "scripts": {
    "build": "wireit",
    "deploy": "wireit",
    "dev": "wireit",
    "format": "wireit",
    "lint": "wireit",
    "prebuild": "wireit",
    "predeploy": "wireit",
    "serve": "wireit",
    "test:coverage": "wireit",
    "test:all": "wireit",
    "test:watch": "wireit",
    "type-check": "wireit",
    "postinstall": "wireit",
    "prepack": "wireit",
    "postpack": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "type-check"
      ],
      "command": "vite build"
    },
    "deploy": {
      "command": "gh-pages -d build"
    },
    "dev": {
      "command": "vite"
    },
    "format": {
      "command": "prettier --write",
      "files": [
        "\"src/**/*.{js,jsx,ts,tsx}\""
      ]
    },
    "lint": {
      "command": "eslint",
      "files": [
        "\"src/**/*.{js,jsx,ts,tsx}\" --fix"
      ]
    },
    "prebuild": {
      "dependencies": [
        "lint",
        "format"
      ]
    },
    "predeploy": {
      "dependencies": [
        "build"
      ]
    },
    "serve": {
      "command": "vite preview"
    },
    "test:coverage": {
      "command": "jest --coverage"
    },
    "test:all": {
      "dependencies": [
        "test:coverage"
      ]
    },
    "test:watch": {
      "command": "jest --watch"
    },
    "type-check": {
      "command": "tsc --noEmit"
    },
    "type-check:watch": {
      "command": "tsc --noEmit --watch"
    },
    "postinstall": {
      "command": "husky install"
    },
    "prepack": {
      "command": "pinst --disable"
    },
    "postpack": {
      "command": "pinst --enable"
    }
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "vite": "^5.0.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.4.1",
    "@types/node": "^20.10.4",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-testing-library": "^6.2.0",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.3",
    "inquirer": "^9.2.12",
    "jest": "^27.5.1",
    "jest-extended": "^4.0.2",
    "pinst": "^3.0.0",
    "prettier": "^3.1.1",
    "process": "^0.11.10",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.4",
    "tsx": "^4.6.2",
    "typescript": "^4.9.5",
    "vite-plugin-eslint": "^1.8.1",
    "wireit": "^0.14.1"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
